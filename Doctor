package com.msyshospital;

public class DoctorImpl{
	
	
	String docName;
	int docAge;
	String docProfession;
	int docExp;
	
	
	
	public String getDocName() {
		
		return docName;
	}
	
	public void setDocName(String docName) {
		
		this.docName = docName;
	}
	
	
	
	
	public int getDocAge() {
		
		return docAge;
	}
	public void setDocAge(int docAge) {
		this.docAge = docAge;
		
	}
	 
	
	public String getDocProfession() {
		return docProfession;
	}
	public void setDocProfession(String docProfession) {
		this.docProfession = docProfession;
	}
	
	

	public int getDocExp() {
		
		return docExp;
	}
	
	public void setDocExp(int docExp) {
		
		this.docExp = docExp;
	}
	
	
	
	@Override
	public String toString() {
		
		return 
				"DoctorImpl "
				+ "[docName=" 
				+ docName + ","
				+"docAge="		
				+ docAge+","
				+ " docExp=" 
				+ docExp + "]";
	}
}	
	
	
	




